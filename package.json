  },
  "devDependencies": {
    "coveralls": "2.11.12",
    "nsp": "2.6.1",
    "standard": "7.1.2",
    "tap": "6.3.0"
  },
  "dependencies": {
    "seneca": "2.1.0",
    "seneca-balance-client": "0.5.0",
    "seneca-mesh": "0.8.0",
    "tfk-generate-jwt": "1.0.0",
    "wreck": "9.0.0"
